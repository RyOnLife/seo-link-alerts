<h2>New Feed</h2>

<% form_for(@feed) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :domain %><br />
    <%= f.text_field :domain %>
  </p>
  <p>
    <%= f.label :url, 'URL' %><br />
    <%= f.text_field :url, :class => 'url' %>
  </p>  
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back to Feeds &raquo;', feeds_path %>